<% @origination_module.origination_section.kept.each do |section| %>
	<div class="card shadow my-2" data-id="OriginationSection-<%= section.id %>">
		<div class="card-body d-flex justify-content-between">
			<div><%= link_to section.resource.name, edit_form_template_path(section.resource), target: '_blank', class: 'text-primary' %></div>
			<div class="d-flex">
		      	<%= link_to(I18n.t('general.edit'), edit_origination_section_path(section), class: 'text-primary mx-2', remote: true, method: 'get', data: { disable_with: I18n.t('form.please_wait') } ) %>				
		      	<%= link_to(I18n.t('general.destroy'), origination_section_path(section), class: 'text-danger mx-2', remote: true, method: 'delete', data: { disable_with: I18n.t('form.please_wait') } ) %>				
			</div>

		</div>
	</div>
<% end %>
