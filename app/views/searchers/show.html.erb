
<% if @users.count > 0 %>
	<h4 class="mx-4"><%= Student.model_name.human(count: @users.count) %></h4>
	<% searcher_table = current_user.template('Student','students',current_user,id: true) %>
	<% searcher_table[:id][:controller] = 'students' %>
	<% searcher_table[:id][:action] = 'edit' %>
	<% searcher_table[:id][:target_id] = 'id' %>
	<% searcher_table[:identification_number] = {} %>
	<%= createTable(@users,searcher_table,id: 'table_identification_number',title: false,new: false).html_safe %>
<% end %>

