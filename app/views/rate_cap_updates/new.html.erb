<%= form_title(@rate_cap_update,I18n.t('mains.new')) %>
<div class="card  mb-3">
    <h5 class="card-header ">
      <div class="d-flex justify-content-between">
          <%= StudentTokenBatch.model_name.human(count: 1) %>
      </div>
    </h5>
  <div class="card-body">
    <%= form_for @rate_cap_update do |f| %>
    <div class="row">
      <div class="col-md-6">
        <div class="form-group">
           <%= f.hidden_field(:responsible_id, value: params[:responsible_id]) %>
          <%= f.label I18n.t('rate_cap_update.attributes.rate_cap_value') %>
          <%= f.text_field :rate_cap_value, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label I18n.t('rate_cap_update.attributes.country') %>
          <%= f.select(:country, options_for_select([['Colombia', 'Colombia'], ['PerÃº', 'Peru' ]]), class: 'form-control') %>
        </div>
      </div>
    </div>
    <div class='row'>
      <div class="col-md-12">
        <div class="form-group">
          <%= f.submit I18n.t('rate_cap_update.actions.save'), class: 'btn btn-success lumnisubmit pull-right' %>
        </div>
      </div>
    </div>
    <%end%>
  </div>
</div>