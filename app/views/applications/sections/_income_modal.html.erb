
<!-- Modal -->
<div class="modal fade" id="new_income_information" tabindex="-1" role="dialog" aria-labelledby="new_income_informationLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body p-0">
	      <%= render '/applications/partial/general_form', target_info: new_record, section: section, application: application, hidden_fields: hidden_fields, action: 'create_new_income',remote: true, target_template: section.cached_form_template.template_hash( current_user, current_company), target_controller: 'income_informations' %>		
      </div>
    </div>
  </div>
</div>
