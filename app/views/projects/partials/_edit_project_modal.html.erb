<!-- Modal -->
<div class="modal fade" id="edit-project_modal" tabindex="-1" role="dialog" aria-labelledby="edit-project_modalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="edit-project_modalLabel"><%= I18n.t('general.edit') %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body" id='edit_project_modal_body'>
        <%= build_form(project,'update', current_user.template('Project','projects',current_user,current_company: current_company),html: { class: 'form-group', method: 'put'})%>
      </div>
    </div>
  </div>
</div>